FIBONAACCI:

#!/bin/bash

if [ $# -eq 0 ]; then
    echo "Error: No input provided. Please provide the number of terms for the Fibonacci series."
    exit 1
fi

n=$1
a=0
b=1

echo "Fibonacci Series up to $n terms:"
for ((i=0; i<n; i++))
do
    echo -n "$a "
    next=$((a + b))
    a=$b
    b=$next
done

echo
